<header>
	<h2>All Accounts</h2>
</header>
<body>

<table>
<thead>
		<tr>
			<th>Acount Name</th>
			<th>Account Number</th>
			<th>Bank</th>
			<th>Balance</th>
		</tr>
</thead>
<tbody>

<% @accounts.each do |account| %>

<tr>
	<td><%= link_to account.name, account_path(account) %></td>
<td><%= account.account_number %></td>
<td><%= account.bank %></td>
<td><%= account.balance %></td>
<% end %>
</tr>
</tbody>
</table>
<br>
<%= link_to "Create New Account", new_account_path %>
<br>
<p><%= link_to 'Signout', destroy_user_session_path, :method => :delete %></p>
</body>




